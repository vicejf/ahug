            <mainEntityId></mainEntityId>
            <pkFieldId></pkFieldId>
            <billNoFieldId></billNoFieldId>
            <corpFieldId></corpFieldId>
            <busiTypeFieldId></busiTypeFieldId>
            <operatorIdFieldId></operatorIdFieldId>
            <approverFieldId></approverFieldId>
            <billStatusFieldId></billStatusFieldId>
            <approveNoteFieldId></approveNoteFieldId>
            <approveDateFieldId></approveDateFieldId>
            <billDateFieldId></billDateFieldId>
            <billTypeFieldId></billTypeFieldId>
        </metadataIds>
        <referenceIds>
            <!-- 参照模型ID -->
            <refPubBillId></refPubBillId>
            <refUserId></refUserId>
            <refBillStatusId></refBillStatusId>
        </referenceIds>
        <connectionIds>
            <!-- 连接ID -->
            <connectionPubBillId></connectionPubBillId>
            <connectionUserId></connectionUserId>
            <connectionBillStatusId></connectionBillStatusId>
        </connectionIds>
