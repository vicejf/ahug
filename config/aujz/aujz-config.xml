<?xml version="1.0" encoding="UTF-8"?>

<billConfig>
    <basicInfo>
        <!-- 单据基本信息 -->
        <billCode>AUJZ</billCode>
        <billName>物料开封</billName>
        <module></module>
        <packageName>ahu</packageName>
        <bodyCode></bodyCode>
        <headCode>AUJZHVO</headCode>
        <billType>single</billType>
        <description></description>
    </basicInfo>
    <fieldPaths>
        <!-- 字段配置文件路径 -->
        <headFieldsPath>AUJZ.json</headFieldsPath>
        <bodyFieldsPath>AUJZBVO.json</bodyFieldsPath>
    </fieldPaths>
    <globalConfig>
        <generateOptions>
