package nc.ui.${bill.packageName}.${bill.classNameLower};

import nc.ui.bd.ref.AbstractRefModel;

public class ${bill.className}RefModel extends AbstractRefModel{
	
	public ${bill.className}RefModel() {
		super();
		init();
	}
	
	private void init(){
	
		setRefNodeName("${bill.billName}参照");
		setRefTitle("${bill.billName}参照");
		setFieldCode(new String[] { "vcode","custname", });
		setFieldName(new String[] { "客户编号","客户名称"});
		setHiddenFieldCode(new String[] { "pk_${bill.classNameLower}_h"});
