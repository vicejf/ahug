		setRefCodeField("vcode");
		setRefNameField("custname");
		setTableName("ahu_${bill.classNameLower}_h");
		setWherePart("pk_corp='"+getPk_corp()+"' and nvl(dr,0)=0 and nvl(isseal,'N')='N' ");
