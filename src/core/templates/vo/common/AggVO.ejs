package nc.vo.<%= bill.packageName %>.<%= bill.billCode.toLowerCase() %>;

import nc.vo.pub.AggregatedValueObject;
import nc.vo.pub.CircularlyAccessibleValueObject;
import nc.vo.pub.IVOMeta;
import nc.vo.pub.VOUtils;

/**
 * <%= bill.billName %> 聚合VO
 * 创建日期:<%= date %>
 * @author <%= bill.globalConfig?.author || 'Unknown' %>
 * @version NCPrj 1.0
 */
@SuppressWarnings("serial")
public class Agg<%= bill.billCode %>VO extends AggregatedValueObject {

	private static final long serialVersionUID = 1L;

	public Agg<%= bill.billCode %>VO() {
		super();
	}

	@Override
	public CircularlyAccessibleValueObject[] getChildrenVO() {
		return super.getChildrenVO();
	}

	@Override
	public void setChildrenVO(CircularlyAccessibleValueObject[] childrenVO) {
		super.setChildrenVO(childrenVO);
	}

	@Override
	public CircularlyAccessibleValueObject getParentVO() {
		return super.getParentVO();
	}

	@Override
	public void setParentVO(CircularlyAccessibleValueObject parentVO) {
		super.setParentVO(parentVO);
	}

	@Override
	public String getParentPKFieldName() {
		return "<%= bill.headFields[0].name %>";
	}

	@Override
	public String getChildrenPKFieldName() {
		return "<%= bill.bodyFields[0].name %>";
	}

	@Override
	public String getTableName() {
		return "<%= bill.tableName || `ahu_${bill.billCode.toLowerCase()}_h` %>";
	}

	@Override
	public IVOMeta getMetaData() {
		return VOUtils.getVOMeta(this);
	}
}
